@model IEnumerable<Microsoft.AspNetCore.Identity.IdentityUser> // Adjust this namespace according to your project

@{
    ViewData["Title"] = "Admin Dashboard";
}


<div class="row justify-content-center">

    <div class="col-md-4 col-sm-8 text-white">
<h2>Admin Dashboard</h2>

        <table class="table text-white">
    <thead>
        <tr>
            <th>User Name</th>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.UserName</td>
                <td>@user.Email</td>
                <td>
                    <a href="@Url.Action("Edit", "Admin", new { id = user.Id })">Edit</a> |
                    <a href="@Url.Action("Delete", "Admin", new { id = user.Id })">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

</div>
</div>